<odoo>

    <record model="ir.ui.view" id="tms_shipment_form_view">
        <field name="model">tms.shipment</field>
        <field name="inherit_id" ref="tms_shipment.tms_shipment_view_form" />
        <field name="arch" type="xml">
            <xpath
                expr="//field[@name='order_ids']/tree/field[@name='priority']"
                position="before"
            >
                <field name="is_pickup_order" optional="hide" />
            </xpath>
        </field>
    </record>

</odoo>
