<odoo>

    <record model="ir.ui.view" id="tms_route_form_view">
        <field name="model">tms.route</field>
        <field name="inherit_id" ref="tms.view_tms_route_form" />
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="ParÃ¢metros">
                    <group>
                        <group>
                            <field name="region_type" />
                            <field name="type" />
                            <field name="restricted" />
                            <field name="pickup" />
                            <field name="delivery" />
                        </group>
                        <group string="Deadlines and Quantities">
                            <field name="deadline1" />
                            <field name="deadline2" />
                            <field name="quantity" />
                        </group>
                        <group string="Values">
                            <field name="value_td" />
                            <field name="value_su" />
                            <field name="value_pickup" />
                        </group>
                        <field name="square" />
                    </group>
                </page>
            </notebook>
        </field>
    </record>
